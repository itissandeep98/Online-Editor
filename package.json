{
	"name": "code-editor",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@monaco-editor/react": "^4.3.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^6.2.1",
		"react-scripts": "5.0.0",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"format": "prettier --write --check src",
		"start": "react-scripts start",
		"deploy": "npm run build && layer0 deploy",
		"build": "CI=false & GENERATE_SOURCEMAP=false react-scripts build && cp build/index.html build/404.html",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@layer0/cli": "^4.10.2",
		"@layer0/core": "^4.10.2",
		"@layer0/devtools": "^4.10.2",
		"@layer0/prefetch": "^4.10.2",
		"autoprefixer": "^10.4.2",
		"postcss": "^8.4.6",
		"prettier": "^2.5.1",
		"tailwindcss": "^3.0.22"
	}
}
